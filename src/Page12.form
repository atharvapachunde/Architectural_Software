                <Container class="javax.swing.JScrollPane" name="jScrollPane9">
                  <Properties>
                    <Property name="horizontalScrollBarPolicy" type="int" value="31"/>
                  </Properties>
                  <AuxValues>
                    <AuxValue name="autoScrollPane" type="java.lang.Boolean" value="true"/>
